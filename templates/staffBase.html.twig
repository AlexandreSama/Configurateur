<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <link rel="stylesheet" href="{{ asset('assets/staff/index.css')}}">
        <!-- animate CSS -->
        <link rel="stylesheet" href="assets/main/css/animate.css">
        <!-- owl carousel CSS -->
        <link rel="stylesheet" href="assets/main/css/owl.carousel.min.css">
        <!-- font awesome CSS -->
        <link rel="stylesheet" href="assets/main/css/all.css">
        <!-- flaticon CSS -->
        <link rel="stylesheet" href="assets/main/css/flaticon.css">
        <link rel="stylesheet" href="assets/main/css/themify-icons.css">
        <!-- font awesome CSS -->
        <link rel="stylesheet" href="assets/main/css/magnific-popup.css">
        <!-- swiper CSS -->
        <link rel="stylesheet" href="assets/main/css/slick.css">
        <!-- style CSS -->
        <link rel="stylesheet" href="assets/main/css/style.css">
        {% endblock %}

        {% block javascripts %}
            <script src="https://kit.fontawesome.com/eec634434d.js" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
            </script>
            <script src="{{ asset('assets/staff/index.js') }}"></script>
            <!-- jquery plugins here-->
            <script src="assets/main/js/jquery-1.12.1.min.js"></script>
            <!-- popper js -->
            <script src="assets/main/js/popper.min.js"></script>
            <!-- bootstrap js -->
            <script src="assets/main/js/bootstrap.min.js"></script>
            <!-- easing js -->
            <script src="assets/main/js/jquery.magnific-popup.js"></script>
            <!-- swiper js -->
            <script src="assets/main/js/swiper.min.js"></script>
            <!-- swiper js -->
            <script src="assets/main/js/masonry.pkgd.js"></script>
            <!-- particles js -->
            <script src="assets/main/js/owl.carousel.min.js"></script>
            <script src="assets/main/js/jquery.nice-select.min.js"></script>
            <!-- slick js -->
            <script src="assets/main/js/slick.min.js"></script>
            <script src="assets/main/js/jquery.counterup.min.js"></script>
            <script src="assets/main/js/waypoints.min.js"></script>
            <script src="assets/main/js/contact.js"></script>
            <script src="assets/main/js/jquery.ajaxchimp.min.js"></script>
            <script src="assets/main/js/jquery.form.js"></script>
            <script src="assets/main/js/jquery.validate.min.js"></script>
            <script src="assets/main/js/mail-script.js"></script>
            <!-- custom js -->
            <script src="assets/main/js/custom.js"></script>
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
